  "cSpell.words": [
    "LSHIFT",
    "RSHIFT",
    "aeiou",
    "memoizee"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
